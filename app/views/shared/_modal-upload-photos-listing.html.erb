<div class="modal-header">
  <h5 class="modal-title" id="exampleModalLabel">Upload Profile Photo</h5>
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
  <span aria-hidden="true">&times;</span>
  </button>
</div>
  <div class="modal-body">
    <%= form_for @listing, url: listing_upload_photos_path(@listing.id), method: 'post' do |form|%>
    <div class="d-flex flex-row justify-content-center">
      <div  class="form-group">
      <%= form.label "Upload some photos of your home."%>
      <%= form.file_field :photos, multiple: true, class: 'form-control-file' %>
      </div>
      <div  class="form-group">
        <%= form.submit "Upload", class: 'btn btn-secondary'%>
      </div>
    </div>
    <%end%>
  </div>

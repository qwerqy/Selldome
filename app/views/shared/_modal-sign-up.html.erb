<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">Sign Up</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
    </button>
   </div>
  <div class="modal-body">
      <div id="clearance" class="sign-up">

        <%= render partial: '/users/form' %>

      </div>
  </div>
  <div class="modal-footer justify-content-start">
    <div class="other-links">
      <%= link_to 'Sign In', sign_in_path , remote: true, data: { target: '#modal-signin', toggle: 'modal' }, class: 'btn btn-primary'%>
      <%= link_to "Sign in with Google", "/auth/google_oauth2", class: 'btn btn-danger list-inline-item' %>
    </div>
  </div>
</div>
